#include <iostream>
#include <vector>
#include <algorithm>

int main() {
    std::vector<int> vec = {1, 2, 3, 4, 5};
    std::sort(vec.begin(), vec.end());
    if (vec.size() < 2) {
        std::cout << "Vector should have at least two elements" << std::endl;
    } else {
        std::cout << "Second largest element: " << vec[vec.size() - 2] << std::endl;
    }
    return 0;
}
