#include <iostream>
using namespace std;

int main() {
    // 1. IF-ELSE IF-ELSE
    int age;
    cout << "Enter your age: ";
    cin >> age;

    if (age < 13) {
        cout << "You are a child." << endl;
    } else if (age >= 13 && age < 18) {
        cout << "You are a teenager." << endl;
    } else {
        cout << "You are an adult." << endl;
    }
    return 0;
}
